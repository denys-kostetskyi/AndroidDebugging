<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_inspector">

    <fragment
        android:id="@+id/navigation_inspector"
        android:name="com.denyskostetskyi.debugging.presentation.fragment.LayoutInspectorFragment"
        android:label="@string/title_inspector"
        tools:layout="@layout/fragment_layout_inspector" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.denyskostetskyi.debugging.presentation.fragment.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.denyskostetskyi.debugging.presentation.fragment.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>